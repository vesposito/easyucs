                     "aggr_id": {
                        "description": "Ethernet Port - Aggregate ID",
                        "type": "integer",
                        "default": 0,
                        "minimum": 0,
                        "maximum": 4
                     },
                     "user_label": {
                        "description": "Ethernet Port - User Label",
                        "type": "string",
                        "minLength": 0,
                        "maxLength": 128,
                        "pattern": "^[!#%\\s+&()\\*+,\\-\\./:;\\?@\\[\\]_{|}~a-zA-Z0-9]*$"
                     }
                  }
               }
            },
            "fc_ports": {
               "type": "array",

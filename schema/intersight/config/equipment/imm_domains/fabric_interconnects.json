      "serial": {
         "description": "Fabric Interconnect Serial",
         "type": "string"
      },
      "tags": {
         "description": "Fabric Interconnect Tags",
         "type": "array",
         "items": {
            "description": "Fabric Interconnect Tag",
            "type": "object",
            "properties": {
               "key": {
                  "description": "Fabric Interconnect Tag Key",
                  "type": "string",
                  "default": "",
                  "minLength": 1,
                  "maxLength": 128
               },
               "value": {

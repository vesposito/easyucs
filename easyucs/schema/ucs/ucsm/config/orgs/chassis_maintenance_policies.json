      },
      "descr": {
         "description": "Description of Chassis Maintenance Policy",
         "type": "string",
         "default": "",
         "pattern": "^[ !#$%&\\(\\)\\*\\+,\\-\\./:;\\?@\\[\\]_\\{\\|\\}~a-zA-Z0-9]{0,256}$"
      }
   },
   "additionalProperties":false,
   "required":[

                     "minItems": 1  
                  },
                  "uplink_ethernet_port_channels": {
                     "description": "SPAN Session Uplink Ethernet Port Channels",
                     "type": "array",
                     "items": [ 
                        {  
                           "description": "SPAN Session Uplink Ethernet Port Channel",
                           "type": "object",
                           "properties": {
                              "direction":{

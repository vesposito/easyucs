{
   "id":"ucs_system_config_vlans#",
   "$schema":"http://json-schema.org/draft-07/schema#",
   "title":"EasyUCS ucs system config vlans section",
   "description":"EasyUCS ucs system config vlans section JSON schema",
   "type":"object",
   "oneOf": [
      {
         "properties":{
            "comment": {
               "description": "VLAN Comments",
               "type": "string",
               "default": ""
            },
            "name":{
               "description":"VLAN Name",
               "type":"string",
               "default":"",
               "pattern":"^[\\-\\.:_a-zA-Z0-9]{1,32}$"
            },
            "id":{
               "description":"VLAN ID",
               "type":"string",
               "pattern":"^[1-9]$|^[1-9][0-9]$|^[1-9][0-9][0-9]$|^[1-3][0-9][0-9][0-9]$|^40[0-2][0-9]$|^404[8-9]$|^40[5-8][0-9]$|^409[0-3]$"
            },
            "fabric":{
               "description":"VLAN Fabric",
               "type":"string",
               "default":"dual",
               "enum":[
                  "a",
                  "A",
                  "b",
                  "B",
                  "dual"
               ]
            },
            "sharing_type":{
               "description":"VLAN Sharing Type",
               "type":"string",
               "default":"none",
               "enum":[
                  "community",
                  "isolated",
                  "none",
                  "primary"
               ]
            },
            "primary_vlan_name":{
               "description":"VLAN Primary VLAN Name",
               "type":"string"
            },
            "multicast_policy_name":{
               "description":"VLAN Multicast Policy",
               "type":"string"
            },
            "native_vlan":{
               "description":"Set VLAN as Native VLAN",
               "type":"string",
               "default":"no",
               "enum":[
                  "yes",
                  "no"
               ]
            },
            "org_permissions": {
               "description": "Organizations assigned to the VLAN",
               "type": "array",
               "items": {
                  "type": "string",
                  "description": "Organization assigned to the VLAN (full path)",
                  "default": "",
                  "pattern": "^[\\-\\./:_a-zA-Z0-9]{1,128}$"
               },
               "minItems": 1
            }
         },
         "additionalProperties":false,
         "required":[
            "name",
            "id"
         ]
      },
      {
         "properties":{
            "comment": {
               "description": "VLAN Range Comments",
               "type": "string",
               "default": ""
            },
            "prefix":{
               "description":"VLAN Prefix",
               "type":"string",
               "default":"",
               "pattern":"^[\\-\\.:_a-zA-Z0-9]{1,32}$"
            },
            "id_from":{
               "description":"VLAN ID range starting value",
               "type":"string",
               "pattern":"^[1-9]$|^[1-9][0-9]$|^[1-9][0-9][0-9]$|^[1-3][0-9][0-9][0-9]$|^40[0-2][0-9]$|^404[8-9]$|^40[5-8][0-9]$|^409[0-3]$"
            },
            "id_to":{
               "description":"VLAN ID range ending value",
               "type":"string",
               "pattern":"^[1-9]$|^[1-9][0-9]$|^[1-9][0-9][0-9]$|^[1-3][0-9][0-9][0-9]$|^40[0-2][0-9]$|^404[8-9]$|^40[5-8][0-9]$|^409[0-3]$"
            },
            "fabric":{
               "description":"VLAN Fabric",
               "type":"string",
               "default":"dual",
               "enum":[
                  "a",
                  "A",
                  "b",
                  "B",
                  "dual"
               ]
            },
            "sharing_type":{
               "description":"VLAN Sharing Type",
               "type":"string",
               "default":"none",
               "enum":[
                  "community",
                  "isolated",
                  "none",
                  "primary"
               ]
            },
            "primary_vlan_name":{
               "description":"VLAN Primary VLAN Name",
               "type":"string"
            },
            "multicast_policy_name":{
               "description":"VLAN Multicast Policy",
               "type":"string"
            },
            "native_vlan":{
               "description":"Set VLAN as Native VLAN",
               "type":"string",
               "default":"no",
               "enum":[
                  "yes",
                  "no"
               ]
            },
            "organizations": {
               "description": "Organizations assigned to the VLAN",
               "type": "array",
               "items": {
                  "type": "string",
                  "description": "Organization assigned to the VLAN (full path)",
                  "default": "",
                  "pattern": "^[\\-\\./:_a-zA-Z0-9]{1,128}$"
               },
               "minItems": 1
            }
      },
         "additionalProperties":false,
         "required":[
            "prefix",
            "id_from",
            "id_to"
         ]
      }
   ]
}